<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFFFFF"
    tools:context=".MGMT_main.MainActivity">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="98dp"
        android:orientation="vertical">

    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="80dp"
        tools:ignore="MissingConstraints">

        <ImageView
            android:id="@+id/logo_view"
            android:layout_width="150dp"
            android:layout_height="80dp"
            android:contentDescription="@string/trim"
            android:src="@drawable/villive_logo" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="50dp"
            android:layout_marginStart="155dp"
            android:layout_marginTop="25dp"
            android:backgroundTint="#454040"
            android:text="@string/main_mgmt_page"
            android:textColor="#FF454040"
            android:textSize="25sp"
            android:textStyle="bold" />
    </FrameLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="500dp"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="98dp"
        android:orientation="vertical">

        <FrameLayout
            android:layout_width="match_parent"
            android:layout_height="100dp"
            tools:ignore="MissingConstraints"
            tools:layout_editor_absoluteX="0dp"
            tools:layout_editor_absoluteY="80dp">


            <ImageView
                android:id="@+id/background1"
                android:layout_width="390dp"
                android:layout_height="88dp"
                android:layout_gravity="center"
                android:contentDescription="@string/trim"
                app:srcCompat="@drawable/rectangle" />

            <ImageView
                android:id="@+id/iv_tenant"
                android:layout_width="40dp"
                android:layout_height="40dp"
                android:layout_marginStart="35dp"
                android:layout_marginTop="30dp"
                android:contentDescription="@string/trim"
                app:srcCompat="@drawable/people" />

            <TextView
                android:id="@+id/tv_tenant"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="90dp"
                android:layout_marginTop="33dp"
                android:text="@string/main_mgmt_tanent"
                android:textColor="@color/gray"
                android:textSize="20sp"
                android:textStyle="bold" />

            <Button
                android:id="@+id/btn_tanent"
                android:layout_width="390dp"
                android:layout_height="88dp"
                android:layout_gravity="center"
                android:backgroundTint="#00FF0000"
                android:contentDescription="@string/main_mgmt_tanent" />


        </FrameLayout>

        <FrameLayout
            android:layout_width="match_parent"
            android:layout_height="100dp"
            tools:ignore="MissingConstraints"
            tools:layout_editor_absoluteX="0dp"
            tools:layout_editor_absoluteY="80dp">

            <ImageView
                android:id="@+id/background2"
                android:layout_width="390dp"
                android:layout_height="88dp"
                android:layout_gravity="center"
                android:contentDescription="@string/trim"
                app:srcCompat="@drawable/rectangle" />

            <ImageView
                android:id="@+id/iv_notice"
                android:layout_width="40dp"
                android:layout_height="40dp"
                android:layout_marginStart="35dp"
                android:layout_marginTop="30dp"
                android:contentDescription="@string/trim"
                app:srcCompat="@drawable/notice" />

            <TextView
                android:id="@+id/tv_notice"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="90dp"
                android:layout_marginTop="33dp"
                android:text="@string/main_mgmt_addnotice"
                android:textColor="@color/gray"
                android:textSize="20sp"
                android:textStyle="bold" />

            <Button
                android:id="@+id/btn_notice"
                android:layout_width="390dp"
                android:layout_height="88dp"
                android:layout_gravity="center"
                android:backgroundTint="#00FF0000"
                android:contentDescription="@string/add_notice" />
        </FrameLayout>

        <FrameLayout
            android:layout_width="match_parent"
            android:layout_height="100dp"
            tools:ignore="MissingConstraints"
            tools:layout_editor_absoluteX="0dp"
            tools:layout_editor_absoluteY="80dp">

            <ImageView
                android:id="@+id/background3"
                android:layout_width="390dp"
                android:layout_height="88dp"
                android:layout_gravity="center"
                android:contentDescription="@string/trim"
                app:srcCompat="@drawable/rectangle" />

            <ImageView
                android:id="@+id/iv_complain"
                android:layout_width="40dp"
                android:layout_height="40dp"
                android:layout_marginStart="35dp"
                android:layout_marginTop="30dp"
                android:contentDescription="@string/trim"
                app:srcCompat="@drawable/complain_solve" />

            <TextView
                android:id="@+id/tv_complain"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="90dp"
                android:layout_marginTop="33dp"
                android:text="@string/main_mgmt_complain"
                android:textColor="@color/gray"
                android:textSize="20sp"
                android:textStyle="bold" />

            <Button
                android:id="@+id/btn_complain"
                android:layout_width="390dp"
                android:layout_height="88dp"
                android:layout_gravity="center"
                android:backgroundTint="#00FF0000"
                android:contentDescription="@string/main_mgmt_complain" />
        </FrameLayout>

        <FrameLayout
            android:layout_width="match_parent"
            android:layout_height="100dp"
            tools:ignore="MissingConstraints"
            tools:layout_editor_absoluteX="0dp"
            tools:layout_editor_absoluteY="80dp">

            <ImageView
                android:id="@+id/background4"
                android:layout_width="390dp"
                android:layout_height="88dp"
                android:layout_gravity="center"
                android:contentDescription="@string/trim"
                app:srcCompat="@drawable/rectangle" />

            <ImageView
                android:id="@+id/iv_cost"
                android:layout_width="40dp"
                android:layout_height="40dp"
                android:layout_marginStart="35dp"
                android:layout_marginTop="30dp"
                android:contentDescription="@string/trim"
                app:srcCompat="@drawable/home" />

            <TextView
                android:id="@+id/tv_cost"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="90dp"
                android:layout_marginTop="33dp"
                android:text="@string/main_mgmt_addcost"
                android:textColor="@color/gray"
                android:textSize="20sp"
                android:textStyle="bold" />

            <Button
                android:id="@+id/btn_cost"
                android:layout_width="390dp"
                android:layout_height="88dp"
                android:layout_gravity="center"
                android:backgroundTint="#00FF0000"
                android:contentDescription="@string/main_mgmt_addcost" />
        </FrameLayout>

        <FrameLayout
            android:layout_width="match_parent"
            android:layout_height="100dp"
            tools:ignore="MissingConstraints"
            tools:layout_editor_absoluteX="0dp"
            tools:layout_editor_absoluteY="80dp">

            <ImageView
                android:id="@+id/background5"
                android:layout_width="390dp"
                android:layout_height="88dp"
                android:layout_gravity="center"
                android:contentDescription="@string/trim"
                app:srcCompat="@drawable/rectangle" />

            <ImageView
                android:id="@+id/iv_settings"
                android:layout_width="40dp"
                android:layout_height="40dp"
                android:layout_marginStart="35dp"
                android:layout_marginTop="30dp"
                android:contentDescription="@string/trim"
                app:srcCompat="@drawable/setting_24" />

            <TextView
                android:id="@+id/tv_settings"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="90dp"
                android:layout_marginTop="33dp"
                android:text="@string/settings"
                android:textColor="@color/gray"
                android:textSize="20sp"
                android:textStyle="bold" />

            <Button
                android:id="@+id/btn_settings"
                android:layout_width="390dp"
                android:layout_height="88dp"
                android:layout_gravity="center"
                android:backgroundTint="#00FF0000"
                android:contentDescription="@string/settings" />
        </FrameLayout>

    </LinearLayout>

    </LinearLayout>

    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_gravity="bottom">

        <ImageButton
            android:id="@+id/ibtn_chat"
            android:layout_width="70dp"
            android:layout_height="70dp"
            android:layout_gravity="end|bottom"
            android:layout_marginEnd="20dp"
            android:layout_marginBottom="50dp"
            android:background="@drawable/circle_shape"
            android:contentDescription="@string/chat"
            android:src="@drawable/chat" />

    </FrameLayout>




</androidx.constraintlayout.widget.ConstraintLayout>